@model CodeNest.DTO.Models.WorkspacesDto
@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Home Page";
    var workSpace = HttpContextAccessor.HttpContext?.Session?.GetString("workspaceId");
}

@if (workSpace == null)
{
    @await Html.PartialAsync("Sections/Card/_Card")
}
<script>
    document.addEventListener('DOMContentLoaded', function () {
        var myModal = new bootstrap.Modal(document.getElementById('modalCenter'), {
            keyboard: false
        });
        myModal.show();
    });
</script>
